{
  "name": "@proc7ts/fun-events",
  "version": "9.4.2",
  "description": "Functional event processor",
  "keywords": [
    "dom-events",
    "event",
    "events",
    "event-receiver",
    "event-sender",
    "event-processor",
    "typescript"
  ],
  "homepage": "https://github.com/proc7ts/fun-events",
  "repository": {
    "type": "git",
    "url": "ssh://git@github.com:proc7ts/fun-events.git"
  },
  "license": "MIT",
  "author": "Ruslan Lopatin <ruslan.lopatin@gmail.com>",
  "bugs": {
    "url": "https://github.com/proc7ts/fun-events/issues"
  },
  "type": "module",
  "main": "dist/fun-events.cjs",
  "module": "dist/fun-events.js",
  "types": "index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/fun-events.js",
      "require": "./dist/fun-events.cjs"
    },
    "./dom": {
      "import": "./dist/fun-events.dom.js",
      "require": "./dist/fun-events.dom.cjs"
    }
  },
  "peerDependencies": {
    "@proc7ts/call-thru": "^4.3.1"
  },
  "dependencies": {
    "@proc7ts/primitives": "^1.5.1"
  },
  "devDependencies": {
    "@proc7ts/call-thru": "^4.3.1",
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-node-resolve": "^11.0.0",
    "@run-z/eslint-config": "^1.1.0",
    "@run-z/rollup-helpers": "^1.0.0",
    "@types/jest": "^26.0.16",
    "@typescript-eslint/eslint-plugin": "^4.9.0",
    "@typescript-eslint/parser": "^4.9.0",
    "codecov": "^3.8.1",
    "eslint": "^7.14.0",
    "eslint-plugin-jest": "^24.1.3",
    "gh-pages": "^3.1.0",
    "jest": "^26.6.3",
    "jest-junit": "^12.0.0",
    "rollup": "^2.34.1",
    "rollup-plugin-flat-dts": "^1.0.0",
    "rollup-plugin-sourcemaps": "^0.6.3",
    "rollup-plugin-typescript2": "^0.29.0",
    "run-z": "^1.6.2",
    "shx": "^0.3.3",
    "ts-jest": "^26.4.4",
    "tslib": "^2.0.3",
    "typedoc": "^0.19.0",
    "typedoc-plugin-external-module-name": "^4.0.3",
    "typescript": "^4.1.2"
  },
  "scripts": {
    "all": "run-z build,lint,test",
    "build": "run-z +z --then rollup --config ./rollup.config.js",
    "ci:all": "run-z all +test/--ci/--runInBand",
    "clean": "run-z +z --then shx rm -rf index.d.ts '*/index.d.ts' dist target",
    "doc": "run-z +z --then typedoc",
    "doc:publish": "run-z doc --then gh-pages --dist target/typedoc --dotfiles",
    "lint": "run-z +z --then eslint . --ext .js,.ts",
    "test": "run-z +z --then jest",
    "z": "run-z +build,+doc,+lint,+test"
  }
}

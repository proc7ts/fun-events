{
  "name": "@proc7ts/fun-events",
  "version": "10.5.1",
  "description": "Functional event processor",
  "keywords": [
    "dom-events",
    "event",
    "events",
    "event-receiver",
    "event-sender",
    "event-processor",
    "reactive-events"
  ],
  "homepage": "https://github.com/proc7ts/fun-events",
  "repository": {
    "type": "git",
    "url": "ssh://git@github.com:proc7ts/fun-events.git"
  },
  "license": "MIT",
  "author": "Ruslan Lopatin <ruslan.lopatin@gmail.com>",
  "bugs": {
    "url": "https://github.com/proc7ts/fun-events/issues"
  },
  "type": "module",
  "main": "./dist/fun-events.js",
  "types": "./index.d.ts",
  "exports": {
    ".": "./dist/fun-events.js",
    "./call-thru": "./dist/fun-events.call-thru.js"
  },
  "peerDependencies": {
    "@proc7ts/call-thru": "^4.4.0"
  },
  "peerDependenciesMeta": {
    "@proc7ts/call-thru": {
      "optional": true
    }
  },
  "dependencies": {
    "@proc7ts/primitives": "^3.0.2",
    "@proc7ts/supply": "^1.2.3"
  },
  "devDependencies": {
    "@jest/globals": "^27.3.1",
    "@proc7ts/call-thru": "^4.4.0",
    "@rollup/plugin-node-resolve": "^13.0.6",
    "@run-z/eslint-config": "^2.0.0",
    "@run-z/rollup-helpers": "^1.1.1",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "eslint": "^8.2.0",
    "eslint-plugin-jest": "^25.2.4",
    "gh-pages": "^3.2.3",
    "jest": "^27.3.1",
    "jest-junit": "^13.0.0",
    "jest-mock": "^27.3.0",
    "remark-cli": "^10.0.0",
    "remark-lint-list-item-indent": "^3.1.0",
    "remark-lint-no-shortcut-reference-link": "^3.1.0",
    "remark-preset-lint-recommended": "^6.1.1",
    "rollup": "^2.60.0",
    "rollup-plugin-flat-dts": "^1.3.0",
    "rollup-plugin-sourcemaps": "^0.6.3",
    "rollup-plugin-typescript2": "^0.31.0",
    "run-z": "=1.9.3-bootstrap",
    "shx": "^0.3.3",
    "ts-jest": "^27.0.7",
    "tslib": "^2.3.1",
    "typedoc": "^0.22.9",
    "typescript": "^4.5.2"
  },
  "scripts": {
    "all": "run-z build,lint,test",
    "bootstrap": "rollup -c",
    "build": "run-z +z bootstrap",
    "ci:all": "run-z all +test/--ci/--runInBand",
    "clean": "run-z +z --then shx rm -rf 'index.d.ts?(.map)' '*/index.d.ts?(.map)' dist target",
    "doc": "run-z +z --then typedoc",
    "doc:publish": "run-z doc --then gh-pages --dist target/typedoc --dotfiles",
    "lint": "run-z + lint:md --and eslint .",
    "lint:md": "run-z +z --then remark .",
    "test": "run-z +z env:NODE_OPTIONS=\"--experimental-vm-modules --no-warnings\" --then jest",
    "z": "run-z +cmd:rollup,+cmd:typedoc,+cmd:eslint,+cmd:remark,+cmd:jest"
  }
}

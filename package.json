{
  "name": "@proc7ts/fun-events",
  "version": "10.5.0",
  "description": "Functional event processor",
  "keywords": [
    "dom-events",
    "event",
    "events",
    "event-receiver",
    "event-sender",
    "event-processor",
    "reactive-events"
  ],
  "homepage": "https://github.com/proc7ts/fun-events",
  "repository": {
    "type": "git",
    "url": "ssh://git@github.com:proc7ts/fun-events.git"
  },
  "license": "MIT",
  "author": "Ruslan Lopatin <ruslan.lopatin@gmail.com>",
  "bugs": {
    "url": "https://github.com/proc7ts/fun-events/issues"
  },
  "type": "module",
  "main": "./dist/fun-events.js",
  "types": "./index.d.ts",
  "exports": {
    ".": "./dist/fun-events.js",
    "./call-thru": "./dist/fun-events.call-thru.js"
  },
  "peerDependencies": {
    "@proc7ts/call-thru": "^4.4.0"
  },
  "peerDependenciesMeta": {
    "@proc7ts/call-thru": {
      "optional": true
    }
  },
  "dependencies": {
    "@proc7ts/primitives": "^3.0.0",
    "@proc7ts/supply": "^1.1.0"
  },
  "devDependencies": {
    "@jest/globals": "^27.0.1",
    "@proc7ts/call-thru": "^4.4.0",
    "@rollup/plugin-node-resolve": "^13.0.0",
    "@run-z/eslint-config": "^1.2.1",
    "@run-z/rollup-helpers": "^1.1.1",
    "@typescript-eslint/eslint-plugin": "^4.25.0",
    "@typescript-eslint/parser": "^4.25.0",
    "eslint": "^7.27.0",
    "eslint-plugin-jest": "^24.3.6",
    "gh-pages": "^3.2.0",
    "jest": "^27.0.1",
    "jest-junit": "^12.1.0",
    "jest-mock": "^27.0.1",
    "rollup": "^2.50.4",
    "rollup-plugin-flat-dts": "^1.2.2",
    "rollup-plugin-sourcemaps": "^0.6.3",
    "rollup-plugin-typescript2": "^0.30.0",
    "run-z": "^1.7.2",
    "shx": "^0.3.3",
    "ts-jest": "^27.0.1",
    "tslib": "^2.2.0",
    "typedoc": "^0.20.36",
    "typescript": "^4.3.2"
  },
  "scripts": {
    "all": "run-z build,lint,test",
    "build": "run-z +z --then rollup -c",
    "ci:all": "run-z all +test/--ci/--runInBand",
    "clean": "run-z +z --then shx rm -rf 'index.d.ts?(.map)' '*/index.d.ts?(.map)' dist target",
    "doc": "run-z +z --then typedoc",
    "doc:publish": "run-z doc --then gh-pages --dist target/typedoc --dotfiles",
    "lint": "run-z +z --then eslint . --ext .js,.ts",
    "test": "run-z +z --then jest",
    "z": "run-z +build,+doc,+lint,+test"
  }
}

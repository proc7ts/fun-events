{
  "name": "@proc7ts/fun-events",
  "version": "10.4.0",
  "description": "Functional event processor",
  "keywords": [
    "dom-events",
    "event",
    "events",
    "event-receiver",
    "event-sender",
    "event-processor",
    "reactive-events"
  ],
  "homepage": "https://github.com/proc7ts/fun-events",
  "repository": {
    "type": "git",
    "url": "ssh://git@github.com:proc7ts/fun-events.git"
  },
  "license": "MIT",
  "author": "Ruslan Lopatin <ruslan.lopatin@gmail.com>",
  "bugs": {
    "url": "https://github.com/proc7ts/fun-events/issues"
  },
  "type": "module",
  "main": "./dist/fun-events.cjs",
  "module": "./dist/fun-events.js",
  "types": "./index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/fun-events.js",
      "require": "./dist/fun-events.cjs"
    },
    "./call-thru": {
      "import": "./dist/fun-events.call-thru.js",
      "require": "./dist/fun-events.call-thru.cjs"
    }
  },
  "peerDependencies": {
    "@proc7ts/call-thru": "^4.3.1"
  },
  "peerDependenciesMeta": {
    "@proc7ts/call-thru": {
      "optional": true
    }
  },
  "dependencies": {
    "@proc7ts/primitives": "^2.0.0",
    "@proc7ts/supply": "^1.0.0"
  },
  "devDependencies": {
    "@proc7ts/call-thru": "^4.3.1",
    "@rollup/plugin-commonjs": "^17.1.0",
    "@rollup/plugin-node-resolve": "^11.2.1",
    "@run-z/eslint-config": "^1.2.1",
    "@run-z/rollup-helpers": "^1.1.0",
    "@types/jest": "^26.0.22",
    "@typescript-eslint/eslint-plugin": "^4.19.0",
    "@typescript-eslint/parser": "^4.19.0",
    "eslint": "^7.23.0",
    "eslint-plugin-jest": "^24.3.2",
    "gh-pages": "^3.1.0",
    "jest": "^26.6.3",
    "jest-junit": "^12.0.0",
    "rollup": "^2.43.1",
    "rollup-plugin-flat-dts": "^1.2.0",
    "rollup-plugin-sourcemaps": "^0.6.3",
    "rollup-plugin-typescript2": "^0.30.0",
    "run-z": "^1.7.1",
    "shx": "^0.3.3",
    "ts-jest": "^26.5.4",
    "tslib": "^2.1.0",
    "typedoc": "^0.20.34",
    "typescript": "^4.2.3"
  },
  "scripts": {
    "all": "run-z build,lint,test",
    "build": "run-z +z --then rollup --config ./rollup.config.js",
    "ci:all": "run-z all +test/--ci/--runInBand",
    "clean": "run-z +z --then shx rm -rf 'index.d.ts?(.map)' '*/index.d.ts?(.map)' dist target",
    "doc": "run-z +z --then typedoc",
    "doc:publish": "run-z doc --then gh-pages --dist target/typedoc --dotfiles",
    "lint": "run-z +z --then eslint . --ext .js,.ts",
    "test": "run-z +z --then jest",
    "z": "run-z +build,+doc,+lint,+test"
  }
}
